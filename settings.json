{
    "security.workspace.trust.untrustedFiles": "open",
    "editor.inlineSuggest.suppressSuggestions": true,
    "workbench.startupEditor": "none",
    "explorer.compactFolders": false,
    "editor.mouseWheelZoom": true,
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "window.zoomLevel": 0,
    "terminal.integrated.zoom": 1,
    "terminal.integrated.scrollWheelZoom": true,
    "code-runner.executorMap": {

        "javascript": "clear ; node",
        "java": "clear ; cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "clear ; cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "zig": "clear ; zig run",
        "cpp": "clear ; cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "clear ; cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "clear ; php",
        "python": "clear ; python -u",
        "perl": "clear ; perl",
        "perl6": "clear ; perl6",
        "ruby": "clear ; ruby",
        "go": "clear ; go run",
        "lua": "clear ; lua",
        "groovy": "clear ; groovy",
        "powershell": "clear ; powershell -ExecutionPolicy ByPass -File",
        "bat": "clear ; cmd /c",
        "shellscript": "clear ; bash",
        "fsharp": "clear ; fsi",
        "csharp": "clear ; scriptcs",
        "vbscript": "clear ; cscript //Nologo",
        "typescript": "clear ; ts-node",
        "coffeescript": "clear ; coffee",
        "scala": "clear ; scala",
        "swift": "clear ; swift",
        "julia": "clear ; julia",
        "crystal": "clear ; crystal",
        "ocaml": "clear ; ocaml",
        "r": "clear ; Rscript",
        "applescript": "clear ; osascript",
        "clojure": "clear ; lein exec",
        "haxe": "clear ; haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "clear ; cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "clear ; racket",
        "scheme": "clear ; csi -script",
        "ahk": "clear ; autohotkey",
        "autoit": "clear ; autoit3",
        "dart": "clear ; dart",
        "pascal": "clear ; cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "clear ; cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "clear ; runghc",
        "nim": "clear ; nim compile --verbosity:0 --hints:off --run",
        "lisp": "clear ; sbcl --script",
        "kit": "clear ; kitc --run",
        "v": "clear ; v run",
        "sass": "clear ; sass --style expanded",
        "scss": "clear ; scss --style expanded",
        "less": "clear ; cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "clear ; cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "clear ; cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "clear ; cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "clear ; cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "clear ; cd $dir && sml $fileName",
        "mojo": "clear ; mojo run",
        "erlang": "clear ; escript",
        "spwn": "clear ; spwn build",
        "pkl": "clear ; cd $dir && pkl eval -f yaml $fileName -o $fileNameWithoutExt.yaml",
        "gleam": "clear ; gleam run -m $fileNameWithoutExt"
    },
    "code-runner.showRunCommandInEditorContextMenu": true,
    "workbench.iconTheme": "material-icon-theme",
    "code-runner.ignoreSelection": true,
    "python.defaultInterpreterPath": "python",
    "git.autofetch": true,
    "git.confirmSync": false,
    "chat.commandCenter.enabled": false,
    "github.copilot.editor.enableAutoCompletions": false,
    "github.copilot.chat.agent.runTasks": false,
    "github.copilot.chat.codeGeneration.useInstructionFiles": false,
    "github.copilot.chat.fixTestFailure.enabled": false,
    "github.copilot.chat.useProjectTemplates": false,
    "github.copilot.editor.enableCodeActions": false,
    "github.copilot.renameSuggestions.triggerAutomatically": false,
    "github.copilot.chat.copilotDebugCommand.enabled": false,
    "github.copilot.chat.reviewSelection.enabled": false,
    "github.copilot.chat.startDebugging.enabled": false,
    "github.copilot.chat.edits.suggestRelatedFilesForTests": false,
    "github.copilot.chat.edits.suggestRelatedFilesFromGitHistory": false,
    "github.copilot.chat.setupTests.enabled": false,
    "chat.experimental.statusIndicator.enabled": false,
    "git.enableSmartCommit": true,
    "workbench.colorTheme": "Default Italic",
    "editor.renderWhitespace": "all"
    }
