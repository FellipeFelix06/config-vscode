{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "root <#ffffff>in</> ",
          "type": "root"
        },
        {
          "foreground": "#55B9C4",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#C678DD",
          "properties": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD | replace \"\" \" \" }} <#FF0000>[?]</>",
          "type": "git"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#FFD43B",
          "properties": {
            "display_mode": "context",
            "fetch_version": true,
            "display_virtual_env": false,
            "folder_name_fallback": true
          },
          "template": "{{ if .Venv }} <#ffffff>via</> {{ .Venv }} {{ end }}"
        },
        {
          "type": "npm",
          "style": "plain",
          "foreground": "#FFD43B",
          "properties": {
            "display_mode": "context",
            "fetch_version": true,
            "display_virtual_env": false
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#FFD43B",
          "template": "<#ffffff>via</> {{ .Version }}"
        },
        {
          "foreground": "#C94A16",
          "style": "plain",
          "template": "x ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#63F08C",
          "style": "plain",
          "template": "\u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
